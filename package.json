{
  "name": "mothership",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "date-fns": "^1.30.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.5",
    "react-table": "^6.9.2",
    "reactjs-popup": "^1.3.2",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "npm run watch:css & react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "build:css": "postcss src/styles/index.css -o src/index.css && npm run purgecss",
    "watch:css": "postcss src/styles/index.css -o src/index.css -w",
    "eject": "react-scripts eject",
    "purgecss": "purgecss -c purgecss.config.js -o src"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "postcss-cli": "^6.1.2",
    "redux-logger": "^3.0.6",
    "tailwindcss": "^0.7.4"
  }
}
